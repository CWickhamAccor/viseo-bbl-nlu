{
    "extends": ["airbnb", "plugin:jest/recommended"],
    "rules": {
    "func-names": ["error", "never"],
        "no-underscore-dangle": "off",
        "indent": [ 2, 4, {"SwitchCase": 1}],
        "max-len": [1, 120, 4],
        "no-param-reassign": [2, {"props": false}],
        "linebreak-style": 0
    }
}